<%- include('../superPartials/head.partials.ejs')%>

    <!-- Page Preloder -->
    <%- include('../superPartials/preloader.partials.ejs')%>
    <!-- Page Preloder -->

    <!-- Offcanvas Menu Begin -->
    <%- include('../superPartials/canvas.partials.ejs')%>
    <!-- Offcanvas Menu End -->

    <!-- Header Section Begin -->
    <%- include("../superPartials/nav")%>
    <!-- Header Section End -->

    <!-- Breadcrumb Begin -->
    <div class="breadcrumb-option">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <div class="breadcrumb__links">
                            <a href="./"><i class="fa fa-home"></i> Home</a>
                            <%if(!displayCategories){%>
                                <span>Shop</span>
                            <%}else{%>
                                <a class="breadcrumb__links text-capitalize" >Shop</a>
                                <a class="breadcrumb__links text-capitalize" ><%= category%></a>
                                <span class="text-capitalize"><%= item %></span>
                            <%}%>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- Breadcrumb End -->

    <!-- Shop Section Begin -->
    <section class="shop spad">
        <div class="container">
            <div class="row">
                <%- include("./partials/sidebar.partials.ejs"); %>

                <%- include("./partials/productList.partials.ejs"); %>
            </div>
        </div>
    </section>
    <!-- Shop Section End -->

    <!-- Search Begin -->
    <%- include("../superPartials/search.partials.ejs"); %>
    <!-- Search End -->

    <!-- Footer Section Begin -->
    <%- include("../superPartials/footer"); %>
    <!-- Footer Section End -->
    </body>

    </html>